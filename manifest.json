{
  "manifest_version": 3,
  "name": "YT Ad Speedup",
  "version": "1.1.2",
  "description": "Speeds up YT ads",
  
  "permissions": [
    "storage",
    "tabs"
  ],
  
"host_permissions": [
  "*://www.youtube.com/*",
  "*://youtube.com/*",
  "*://www.youtube-nocookie.com/*",
  "*://youtube-nocookie.com/*",
  "*://music.youtube.com/*"
],

    "background": {
    "service_worker": "background.js"
  },
    
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "YT Ad Speedup"
  },

    "content_scripts": [
    {
      "matches": [
      "*://www.youtube.com/*",
      "*://youtube.com/*",
      "*://www.youtube-nocookie.com/*",
      "*://youtube-nocookie.com/*",
      "*://music.youtube.com/*"
    ]
      }]
  
}